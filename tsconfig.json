{
  "compilerOptions": {
    "esModuleInterop": true,
    "declaration": true,
    "strict": true,
    "rootDir": "./src",
    "noImplicitAny": false,
    "moduleResolution": "node"
  },
  "include": [
    "./src/**/*",
  ],
  "exclude": [
    "node_modules",
    "src/__tests__/**/*",
    "src/testUtils/**/*"
  ]
}
